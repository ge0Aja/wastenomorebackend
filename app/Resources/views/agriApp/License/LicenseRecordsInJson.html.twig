{% set LicenseRecordsArrayLength = LicenseRecords |length %}
{% set counter = 0 %}



{

"data":[

{% for LicenseRecord in LicenseRecords %}

    {%  if counter == LicenseRecordsArrayLength-1 %}
        {
        "id": "{{ LicenseRecord.id }}",
        "license": "{{ LicenseRecord.license}}",
        "userCount" : "{{ LicenseRecord.userCount }}",
        "startDate" : "{{ LicenseRecord.startDate|date('d-m-Y')}}",
        "expiryDate" : "{{ LicenseRecord.expiryDate|date('d-m-Y')}}",
        "usersOnLicense" : "{{LicenseRecord.userCount - LicenseRecord.licenseUser.count }}"
        }
    {%  else %}
        {
        "id": "{{ LicenseRecord.id }}",
        "license": "{{ LicenseRecord.license}}",
        "userCount" : "{{ LicenseRecord.userCount }}",
        "startDate" : "{{ LicenseRecord.startDate|date('d-m-Y')}}",
        "expiryDate" : "{{ LicenseRecord.expiryDate|date('d-m-Y')}}",
        "usersOnLicense" : "{{LicenseRecord.userCount - LicenseRecord.licenseUser.count }}"
        },
    {% endif %}

    {% set counter = counter + 1 %}
{% endfor %}

]
}
