{% set surveyQsRecordsArrayLength = surveyQsRecords |length %}
{% set counter = 0 %}



{

"data":[

{% for surveyQsRecord in surveyQsRecords %}

    {%  if counter == surveyQsRecordsArrayLength-1 %}
        {
        "id": "{{ surveyQsRecord.id }}",
        "question": "{{ surveyQsRecord.question}}",
        "withdropdown": "{{ surveyQsRecord.questionwithdropdown }}",
        "withdetails": "{{ surveyQsRecord.questionwithdetails }}",
        "dropdowntable" :{% if surveyQsRecord.dropdowntable != null %} "{{ surveyQsRecord.dropdowntable.name }}" {% else %} " " {% endif %},
        "detailshint" : "{{ surveyQsRecord.detailshint }}"

        }
    {%  else %}
        {
        "id": "{{ surveyQsRecord.id }}",
        "question": "{{ surveyQsRecord.question}}",
        "withdropdown": "{{ surveyQsRecord.questionwithdropdown }}",
        "withdetails": "{{ surveyQsRecord.questionwithdetails }}",
        "dropdowntable" :{% if surveyQsRecord.dropdowntable != null %} "{{ surveyQsRecord.dropdowntable.name }}" {% else %} " " {% endif %},
        "detailshint" : "{{ surveyQsRecord.detailshint }}"
        },
    {% endif %}

    {% set counter = counter + 1 %}
{% endfor %}

]
}
