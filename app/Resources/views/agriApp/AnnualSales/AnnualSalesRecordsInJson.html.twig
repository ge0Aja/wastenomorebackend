{% set AnnualSalesRecordsArrayLength = AnnualSalesRecords |length %}
{% set counter = 0 %}



{

"data":[

{% for AnnualSalesRecord in AnnualSalesRecords %}

    {%  if counter == AnnualSalesRecordsArrayLength-1 %}
        {
        "id": "{{ AnnualSalesRecord.id }}",
        "range": "{{ AnnualSalesRecord.salesRange}}"

        }
    {%  else %}
        {
        "id": "{{ AnnualSalesRecord.id }}",
        "range": "{{ AnnualSalesRecord.salesRange}}"
        },
    {% endif %}

    {% set counter = counter + 1 %}
{% endfor %}

]
}
